{
  "name": "capacitor-video-webview",
  "version": "1.0.0",
  "description": "WebView optimizado para videollamadas basado en Air Doctor",
  "main": "dist/plugin.cjs.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "unpkg": "dist/plugin.js",
  "files": [
    "android/src/main/",
    "android/build.gradle",
    "dist/",
    "ios/Sources",
    "ios/Package.swift",
    "CapacitorVideoWebview.podspec"
  ],
  "scripts": {
    "verify": "npm run verify:ios && npm run verify:android && npm run verify:web",
    "verify:ios": "cd ios && pod lib lint CapacitorVideoWebview.podspec --allow-warnings",
    "verify:android": "cd android && ./gradlew clean build test",
    "verify:web": "npm run build",
    "lint": "npm run eslint && npm run prettier -- --check && npm run swiftlint -- lint",
    "fmt": "npm run eslint -- --fix && npm run prettier -- --write && npm run swiftlint -- --fix --format",
    "eslint": "eslint . --ext .ts,.js",
    "prettier": "prettier \"**/*.{css,html,ts,js,java}\"",
    "swiftlint": "node-swiftlint",
    "docgen": "docgen --api CapacitorVideoWebviewPlugin --output-readme README.md --output-json dist/docs.json",
    "build": "npm run clean && tsc && rollup -c rollup.config.js",
    "clean": "rimraf ./dist",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "capacitor",
    "plugin",
    "webview",
    "video",
    "videocalls",
    "telemedicine",
    "webrtc"
  ],
  "author": "CLata",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/clata/capacitor-video-webview.git"
  },
  "bugs": {
    "url": "https://github.com/clata/capacitor-video-webview/issues"
  },
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    }
  },
  "dependencies": {
    "@capacitor/android": "^7.4.3",
    "@capacitor/core": "^7.4.3",
    "@capacitor/ios": "^7.4.3"
  },
  "devDependencies": {
    "@capacitor/cli": "^7.4.3",
    "@capacitor/docgen": "^0.2.2",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@types/node": "^24.3.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.2",
    "typescript": "^5.9.2"
  }
}
